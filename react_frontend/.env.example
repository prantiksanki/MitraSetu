GEMINI_API_KEY=your_google_gemini_api_key_here
# Base URL for backend proxy (used to build LIVE_TOKEN_ENDPOINT)
VITE_API_BASE=http://localhost:5001
PORT=5001
VITE_LIVE_MODE=mock
# Set VITE_LIVE_MODE=mock to always use demo streaming.
# Omit or set to 'live' to attempt real connection then auto-fallback to mock.

# WebSocket live model configuration
VITE_GEMINI_WS_HOST=generativelanguage.googleapis.com
VITE_GEMINI_WS_PATH=/v1beta/models/gemini-2.5-flash-preview-native-audio-dialog:live
VITE_GEMINI_MODEL=models/gemini-2.5-flash-preview-native-audio-dialog
VITE_AUDIO_SAMPLE_RATE=16000
